@model DescriptivePaperDTO
@{
    ViewData["Title"] = "DescriptivePaperDetails";
    var pdfURL = $@"{Model.PaperPdfUrl}#toolbar=0&navpanes=0&scrollbar=0";
}


@await Component.InvokeAsync("CurrPath", new
{
    header = "Descriptive Paper Details",
    paths = new List<EPath>() { EPath.DashBoard, EPath.PaperList, EPath.DescriptivePaperDetail }
})
<object id="pdfFrame" width="100%" height="700px"></object>
<div class="my-3">
    <a asp-action="@nameof(TeacherController.Index)">Back To List</a>
</div>

@section Scripts{
    <script>
        $(document).ready(function () {
            var pdfUrl = "@Html.Raw(System.Web.HttpUtility.JavaScriptStringEncode(pdfURL))";
            $("#pdfFrame").attr("data", pdfUrl);
        });
    </script>
}